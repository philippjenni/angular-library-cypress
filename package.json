{
  "name": "angular-coverage",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "build-lib": "ng build --project ComponentLib",
    "cypress-lib-run": "npx cypress run --component --project projects/component-lib --env coverage=true",
    "cypress-lib-edit": "npx cypress open --project projects/component-lib --env coverage=true",
    "cypress-project-run": "npx cypress run --component --env coverage=true",
    "cypress-project-edit": "npx cypress open --env coverage=true",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.0",
    "@angular/common": "~13.1.0",
    "@angular/compiler": "~13.1.0",
    "@angular/core": "~13.1.0",
    "@angular/forms": "~13.1.0",
    "@angular/platform-browser": "~13.1.0",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "@angular/router": "~13.1.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.3",
    "@angular/cli": "~13.1.3",
    "@angular/compiler-cli": "~13.1.0",
    "@types/node": "^12.11.1",
    "cypress": "^12.5.1",
    "ng-packagr": "^13.0.0",
    "typescript": "~4.5.2"
  }
}
